---
layout: empty
title : RSS
group: navigation
production_url: https://truongtx.me
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }}</title>
 <link href="{{ page.production_url }}/atom.xml" rel="self"/>
 <link href="{{ page.production_url }}"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ page.production_url }}</id>
 <author>
   <name>{{ site.author.name }}</name>
   <email>{{ site.author.email }}</email>
 </author>

 {% for post in site.posts limit:15 %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="{{ page.production_url }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ page.production_url }}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
